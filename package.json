{
	"name": "digitcore",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"precheck": "npm run typecheck && npm run check:write && npm run build",
		"cypress:open": "cross-env NODE_ENV=test SKIP_ENV_VALIDATION=1 cypress open",
		"cypress:run": "cross-env NODE_ENV=test SKIP_ENV_VALIDATION=1 cypress run",
		"dev": "next dev",
		"dev:all": "concurrently \"npm run dev\" \"npm run studio:dev\"",
		"e2e": "start-server-and-test dev http://localhost:3000 \"cross-env NODE_ENV=test SKIP_ENV_VALIDATION=1 cypress run --e2e\"",
		"e2e:dev": "start-server-and-test dev http://localhost:3000 \"cross-env NODE_ENV=test SKIP_ENV_VALIDATION=1 cypress open --e2e\"",
		"e2e:production": "start-server-and-test start http://localhost:3000 \"cypress run --e2e\"",
		"preview": "next build && next start",
		"start": "next start",
		"studio:dev": "sanity dev --cwd ./src/sanity",
		"studio:build": "sanity build --cwd ./src/sanity",
		"studio:deploy": "sanity deploy --cwd ./src/sanity",
		"studio:install": "npm install --workspace ./src/sanity",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"typecheck": "tsc --noEmit",
		"typesense:server": "typesense-server --data-dir=./typesense-data --api-key=xyz --listen-port=8108 --enable-cors &",
		"typesense:stop": "pkill -f typesense-server",
		"typesense:index": "node scripts/index-typesense.js",
		"typesense:setup": "npm run typesense:server && sleep 5 && npm run typesense:index"
	},
	"dependencies": {
		"@hugeicons/core-free-icons": "^1.0.16",
		"@hugeicons/react": "^1.0.5",
		"@radix-ui/react-accordion": "^1.2.11",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-collapsible": "^1.1.11",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-hover-card": "^1.1.14",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-toggle": "^1.1.9",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@react-pdf/renderer": "^4.3.0",
		"@sanity/icons": "^3.7.4",
		"@sanity/image-url": "^1.1.0",
		"@sanity/vision": "^4.3.0",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"botid": "^1.4.5",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"dotenv": "^17.2.1",
		"lucide-react": "^0.525.0",
		"motion": "^12.23.12",
		"next": "^15.4.6",
		"next-sanity": "^10.0.10",
		"resend": "^2.0.0",
		"next-themes": "^0.4.6",
		"radix-ui": "^1.4.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"sanity": "^4.3.0",
		"server-only": "^0.0.1",
		"sonner": "^2.0.6",
		"styled-components": "^6.1.19",
		"superjson": "^2.2.1",
		"tailwind-merge": "^3.3.1",
		"typesense": "^2.1.0-13",
		"typesense-instantsearch-adapter": "^2.9.0",
		"use-debounce": "^10.0.5",
		"zod": "^3.24.2",
		"zustand": "^5.0.6"
	},

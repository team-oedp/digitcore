		"cypress": "^14.5.2",
		"jsdom": "^24.0.0",
		"postcss": "^8.5.3",
		"start-server-and-test": "^2.0.12",
		"tailwindcss": "^4.0.15",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.2",
		"vite": "^5.2.0",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^1.5.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "npm@10.2.4"
}
